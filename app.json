{
  "expo": {
    "name": "College Calendar",
    "slug": "college-calendar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/App Logo.jpg",
    "splash": {
      "image": "./assets/images/App Logo - Transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.collegecalendar.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/App Logo - Transparent.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.collegecalendar.app"
    },
    "web": {
      "favicon": "./assets/images/App Logo - Transparent.png"
    }
  }
}
